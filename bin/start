#! /usr/bin/env node

var router = require("../");

var server;
var port = 33750;
var args = process.argv;

if(args[2] && /^\d+$/g.test(args[2])){
    port = args[2];
}

router.init({debug:true}).setMap('**/*' , "**/*")

router.listen(port , function(err){
    if(err.message=="listen EADDRINUSE"){
        this.listen(0)
    }
});
